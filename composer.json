{
    "name": "sindaniel/laravel-telegram-error-reporter",
    "description": "Laravel package to report application errors to Telegram",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "sindaniel",
            "email": "sindanielweb@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sindaniel\\LaravelTelegramErrorReporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sindaniel\\LaravelTelegramErrorReporter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sindaniel\\LaravelTelegramErrorReporter\\TelegramErrorReporterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}